{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "test": {
      "dependsOn": ["^build"],
      "cache": false
    },
    "lint": {
      "outputs": []
    },
    "lint:types": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "generate:docs": {
      "outputs": ["docs/**"]
    }
  }
}
